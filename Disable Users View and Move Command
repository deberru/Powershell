Get-ADComputer -Filter "Name -like 'UCK-*'" -SearchBase "CN=Computers,DC=mikanetwork,DC=com" | Move-ADObject -TargetPath "OU=Ucak Bilet Dept,OU=DC2 Genel Mudurluk,OU=CLIENT,OU=TATILBUDUR,DC=mikanetwork,DC=com"



Search-ADAccount -AccountDisabled | where {$_.ObjectClass -eq ‘user’} | FT Name,ObjectClass –A



/*Disabe Userları bulup, Oluşturduğumuz  Disable Grubuna Taşıyan kod

Search-ADAccount -AccountDisabled -SearchBase "DC=mikanetwork,DC=com" | Move-ADObject -TargetPath "OU=Disable User,OU=USER,OU=TATILBUDUR,DC=mikanetwork,DC=com"
